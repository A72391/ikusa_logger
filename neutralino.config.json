{
	"applicationId": "js.neutralino.zero",
	"version": "1.0.0",
	"defaultMode": "window",
	"documentRoot": "/ui/build/",
	"url": "/",
	"enableServer": true,
	"enableNativeAPI": true,
	"nativeAllowList": ["app.*", "filesystem.*", "os.*"],
	"modes": {
		"window": {
			"title": "Combat Logger",
			"width": 500,
			"height": 400,
			"minWidth": 500,
			"minHeight": 400,
			"maxWidth": 500,
			"maxHeight": 400,
			"icon": "/ui/static/logo.png"
		}
	},
	"cli": {
		"binaryName": "application",
		"resourcesPath": "/ui/build/",
		"extensionsPath": "/extensions/",
		"binaryVersion": "4.10.0",
		"clientVersion": "3.8.2",
		"frontendLibrary": {
			"patchFile": "/ui/src/app.html",
			"devUrl": "http://localhost:5173"
		}
	}
}
